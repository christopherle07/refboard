{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "EyeDea",
  "version": "1.1.0",
  "identifier": "net.eyeda.eye",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "EyeDea",
        "fullscreen": false,
        "maximized": true,
        "dragDropEnabled": false,
        "decorations": false,
        "transparent": false,
        "minWidth": 1000,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["$APPDATA/**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/256x256.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}