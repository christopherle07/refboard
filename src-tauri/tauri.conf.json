{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AniRef",
  "version": "1.3.0",
  "identifier": "site.anihaven.aref",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "AniRef",
        "fullscreen": false,
        "maximized": true,
        "dragDropEnabled": false
        
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/256x256.png",
      "icons/icon.ico"
    ]
  }
}